<div id="full" class="carousel slide"  data-ride="carousel">
	<div class="carousel-inner">
	<% @cycle.each_with_index do |img, index| %>
	  <div class="carousel-item <%= 'active' if index == 0 %>">
	  	<% cycleImg = img.image.url if img.image? %>
	  	<div class="fullimg" style="background-image: url('<%= cycleImg %>'); "></div>
	  </div>
	<% end %>
	</div> <!-- /carousel-inner -->
		<a class="carousel-control-prev" href="#full" role="button" data-slide="prev">
	    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
	    <span class="sr-only">Previous</span>
	  </a>
	  <a class="carousel-control-next" href="#full" role="button" data-slide="next">
	    <span class="carousel-control-next-icon" aria-hidden="true"></span>
	    <span class="sr-only">Next</span>
	  </a>
</div>
